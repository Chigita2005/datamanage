#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int roll_dice() {
    return rand() % 6 + 1; 
}

int main() {
　　　printf("Rolling dice...\n");
    srand(time(NULL)); 

    int dice1 = roll_dice();
    int dice2 = roll_dice();
   printf("Heads: %d, Tails: %d\n",dice1,dice2);

    return 0;

}
